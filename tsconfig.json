{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@types": ["src/types"],
      "@utils": ["src/utils"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
