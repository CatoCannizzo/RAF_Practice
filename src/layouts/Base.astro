---
interface Props {
   title: string;
};
const {title} = Astro.props;
import Navbar from '@components/Navbar.astro'
---
<!DOCTYPE html>
<html>
   <head>
      <meta charset="utf-8">
      <title>{title} / Cannizzo Test</title>
      <meta name="viewport" content="wideth=device-width, initial-scale=1">
      <style>
         @font-face {
            font-family: "Inter";
            src: url("/fonts/Inter-Regular.woff2") format("woff2");
            font-display: swap;
            font-weight: 400;
         }
         @font-face {
            font-family: "Inter";
            src: url("/fonts/Inter-Italic.woff2") format("woff2");
            font-display: swap;
            font-weight: 400;
            font-style: italic;
         }
         @font-face {
            font-family: "Inter";
            src: url("/fonts/Inter-Bold.woff2") format("woff2");
            font-display: swap;
            font-weight: 700;
            font-style: Bold;
         }
         body {
            font-family: Inter, sans-serif;
         }
      </style>
   </head>
   <body>
      <Navbar/>
      <slot/>
   </body>
</html>